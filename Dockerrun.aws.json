{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions":[
        {
            "name":"client",
            "image" :"entityvsentityv2/multi-client",
            "hostname" : "client",
            "essential" : false
        },
        {
            "name":"server",
            "image" :"entityvsentityv2/multi-server",
            "hostname" : "api",
            "essential" : false
        },
        {
            "name":"worker",
            "image" :"entityvsentityv2/multi-worker",
            "hostname" : "worker",
            "essential" : false
        },
        {
            "name":"nginx",
            "image" :"entityvsentityv2/multi-nginx",
            "hostname" : "nginx",
            "essential" : true,
            "portMappings":[
                {
                    "hostPort" : 80,
                    "containerPort" : 80
                }
            ],
            "links" : ["client","server"]
        }
    ]
}